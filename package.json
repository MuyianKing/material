{
  "name": "hl-material",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "lint-staged",
    "build:ui": "vite build  --emptyOutDir --config ./packages/ui/vite.config.js & copy .\\packages\\ui\\package.json .\\base-project\\dist\\hl\\ui & xcopy /E /I .\\packages\\ui\\theme-chalk .\\base-project\\dist\\hl\\ui\\theme-chalk",
    "build:tyyh": "vite build  --emptyOutDir --config ./packages/tyyh/vite.config.js & copy .\\packages\\tyyh\\package.json .\\base-project\\dist\\hl\\tyyh",
    "build:hooks": "vite build  --emptyOutDir --config ./packages/hooks/vite.config.js & copy .\\packages\\hooks\\package.json .\\base-project\\dist\\hl\\hooks",
    "build:utils": "vite build  --emptyOutDir --config ./packages/utils/vite.config.js & copy .\\packages\\utils\\package.json .\\base-project\\dist\\hl\\utils",
    "build:task": "vite build  --emptyOutDir --config ./packages/task/vite.config.js & copy .\\packages\\task\\package.json .\\base-project\\dist\\hl\\task"
  },
  "dependencies": {
    "vue": "^3.4.35"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "cz-git": "^1.9.4",
    "eslint": "9.8.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.6.0",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.7",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
